#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/robin/WebstormProjects/kuklysantonjuk/node_modules/.pnpm/ipx@2.1.0/node_modules/ipx/bin/node_modules:/home/robin/WebstormProjects/kuklysantonjuk/node_modules/.pnpm/ipx@2.1.0/node_modules/ipx/node_modules:/home/robin/WebstormProjects/kuklysantonjuk/node_modules/.pnpm/ipx@2.1.0/node_modules:/home/robin/WebstormProjects/kuklysantonjuk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/robin/WebstormProjects/kuklysantonjuk/node_modules/.pnpm/ipx@2.1.0/node_modules/ipx/bin/node_modules:/home/robin/WebstormProjects/kuklysantonjuk/node_modules/.pnpm/ipx@2.1.0/node_modules/ipx/node_modules:/home/robin/WebstormProjects/kuklysantonjuk/node_modules/.pnpm/ipx@2.1.0/node_modules:/home/robin/WebstormProjects/kuklysantonjuk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/ipx.mjs" "$@"
else
  exec node  "$basedir/../../bin/ipx.mjs" "$@"
fi
